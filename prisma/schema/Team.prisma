model Team {
  key                 Int         @unique
  name                String
  team_matches        TeamMatch[]
  summary             String?      
  scoreRobot          Boolean?
  scoreGrass          Boolean?
  scoreFeedingStation Boolean?
  scoreCabbage        Boolean?
  Autos               Auto[]
  drivetrain          Drivetrain?
}

model Auto {
  id                  Int     @id @default(autoincrement())
  mobility            Boolean
  location            AutoStart
  scoreGrass          Int
  scoreFeedingStation Int
  Team                Team?   @relation(fields: [teamKey], references: [key])
  teamKey             Int?
}

enum AutoStart {
  Far
  Close
  Mid
}
enum Drivetrain {
  Swerve
  Tank
  Other
}
